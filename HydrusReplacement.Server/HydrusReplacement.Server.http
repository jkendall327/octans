@HydrusReplacement.Server_HostAddress = http://localhost:5185
@DefaultFile = https://i.imgur.com/mtbl1cr.jpeg

POST http://{{HydrusReplacement.Server_HostAddress}}/import
Content-Type: application/json

{
  "Items": [
    {
      "Source": "{{DefaultFile}}",
      "Tags": [
        { "Namespace": "foobar", "Subtag": "barfoo" },
        { "Subtag": "foobaz" }
      ]
    }
  ],
  "DeleteAfterImport": false,
  "MinSize": null,
  "MaxSize": 1500000,
  "AllowedFileTypes": [ "jpg", "png" ]
}
